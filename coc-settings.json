{
  "coc.preferences.formatOnSaveFiletypes": [
    "dart",
    "css",
    "json",
    "markdown",
    "python",
    "php",
    "go",
    "typescript",
    "javascript",
    "javascriptreact",
    "typescriptreact"
  ],
  "coc.preferences.hoverTarget": "float",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "go.goplsOptions": {
    "staticcheck": true
  }
}
